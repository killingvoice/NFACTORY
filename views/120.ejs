<html>
    <head>

    </head>
    <body>
        <form method="post" enctype="multipart/form-data" action="/service/additem">
            <select name="property" id="property">
                <% for(var i=0; i<data.length; i++) { %>
                    <option value="<%=data[i].id%>"><%=data[i].id%></option>
                <% } %>
            </select><br>
            <input type="text"   id="id"     name="id"     placeholder="아이템 이름" required/><br>
            <input type="number" id="rarity" name="rarity" placeholder="희귀도" required/><br>
            <input type="file"   id="image"  name="image"  placeholder="이미지" required/><br>
            <input type="submit" />
        </form>
    </body>
</html>